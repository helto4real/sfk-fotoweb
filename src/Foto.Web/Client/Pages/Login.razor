@page "/login"
@inherits BasePage
@layout MainLayout
<LogInForm OnLoggedIn="@OnLoggedIn" Token="@Token"/>

@code {
    [Parameter]
    [SupplyParameterFromQuery(Name = "token")]
    public string Token { get; set; } = String.Empty;
    
    private void OnLoggedIn()
    {
        NavigationManager?.NavigateTo("/", true);
    }
}
